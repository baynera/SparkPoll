package events
{
	import flash.events.Event;
	
	public class GridEvent extends Event
	{
		public static const ON_PUSH_QUESTION:String = "onPushQuestion";
		public static const ON_DELETE_QUESTION:String = "onDeleteQuestion";
		public static const ON_DROP_DOWN:String = "onDropDown";
		public static const ON_PUSH_ENABLE:String = "onPushEnable";

		public function GridEvent(type:String, bubbles:Boolean=true, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}